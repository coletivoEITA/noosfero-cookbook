---
address: 127.0.0.1
port: <%= @site.server.port %>
servers: <%= if @site.server.unicorn? then 0 else @site.server.workers end %>
timeout: <%= @site.server.timeout %>
user: noosfero
log: log/thin.log
pid: tmp/pids/thin.pid
environment: <%= @site.rails.env %>
daemonize: true
chdir: <%= @site.code_path %>
